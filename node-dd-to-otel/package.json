{
  "name": "node-dd-to-otel-demo",
  "version": "1.0.0",
  "description": "Demo Node.js app showing DataDog logs/metrics integration with Cloudflare Worker proxy",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "demo": "node server.js",
    "demo:dev": "node --watch server.js",
    "test": "node test-integration.js",
    "test:server": "node test-server.js",
    "test:otel": "node test-otel-conversion.js",
    "install:all": "npm install && cd worker && npm install",
    "worker:dev": "cd worker && npm run dev",
    "worker:deploy": "cd worker && npm run deploy"
  },
  "dependencies": {
    "@datadog/browser-logs": "^5.24.0",
    "@datadog/browser-rum": "^5.24.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsdom": "^25.0.0",
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "datadog",
    "otel",
    "firetiger",
    "telemetry",
    "proxy"
  ],
  "author": "Rustam Lalkaka <rustam@firetiger.com>",
  "license": "ISC"
}
