{
  "name": "node-dd-to-otel-demo",
  "version": "1.0.0",
  "description": "Demo Node.js app showing DataDog logs/metrics integration with Cloudflare Worker proxy",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "demo": "node server.js",
    "demo:dev": "node --watch server.js",
    "demo:staging": "NODE_ENV=staging node server.js",
    "test": "node tests/test-integration.js",
    "test:server": "node tests/test-server.js",
    "test:otel": "node tests/test-otel-conversion.js",
    "worker:dev": "wrangler dev --env staging",
    "worker:deploy": "wrangler deploy",
    "worker:deploy:staging": "wrangler deploy --env staging"
  },
  "dependencies": {
    "@datadog/browser-logs": "^5.24.0",
    "@datadog/browser-rum": "^5.24.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsdom": "^25.0.0",
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "datadog",
    "otel",
    "firetiger",
    "telemetry",
    "proxy"
  ],
  "author": "Rustam Lalkaka <rustam@firetiger.com>",
  "license": "ISC",
  "devDependencies": {
    "wrangler": "^4.28.1"
  }
}
